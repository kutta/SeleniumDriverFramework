    public void gotoSearchCustomer() throws Exception {
        //this function opens the Consumer home page at its root url.

        System.out.println("entering gotoSearchCustomer");
        Wait waiting = new Wait(callerSelenium);
        waiting.forTextPresent("Search Customer");
        callerSelenium.click("mnu_SEARCH_CUSTOMER");
        waiting.forTextPresent("Search Member");
        System.out.println("end of gotoSearchCustomer");

    }

